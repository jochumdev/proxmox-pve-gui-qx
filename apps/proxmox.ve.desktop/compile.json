{
  "targets": [
    {
      "type": "source",
      "outputPath": "../../source-output",
      "minify": "off"
    },
    {
      "type": "hybrid",
      "outputPath": "../../hybrid-output",
      "minify": "off"
    },
    {
      "type": "build",
      "outputPath": "../../build-output",
      "minify": "off"
    }
  ],
  "defaultTarget": "source",
  "locales": [
    "en",
    "de"
  ],
  "environment": {
    "qx.debug": true,
    "proxmox.debug-routing": false
  },
  "applications": [
    {
      "class": "proxmox.ve.desktop.Application",
      "theme": "proxmox.theme.Theme",
      "name": "desktop",
      "writeIndexHtmlToRoot": false,
      "include": [
        "proxmox.ve.desktop.page.*"
      ]
    }
  ],
  "libraries": [
    "../../../qooxdoo/framework",
    "../../modules/proxmox",
    "."
  ]
}