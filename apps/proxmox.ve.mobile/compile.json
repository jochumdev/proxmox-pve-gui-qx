{
  "targets": [
    {
      "type": "source",
      "outputPath": "../../source-output",
      "minify": "off"
    },
    {
      "type": "hybrid",
      "outputPath": "../../hybrid-output",
      "minify": "off"
    },
    {
      "type": "build",
      "outputPath": "../../build-output",
      "minify": "off"
    }
  ],
  "defaultTarget": "source",
  "locales": [
    "en",
    "de"
  ],
  "environment": {
    "qx.icontheme": "Oxygen",
    "qx.debug": true,
    "proxmox.debug-routing": true
  },
  "applications": [
    {
      "class": "proxmox.ve.mobile.Application",
      "name": "mobile",
      "writeIndexHtmlToRoot": false
    }
  ],
  "libraries": [
    "../../../qooxdoo/framework",
    "../../modules/proxmox",
    "."
  ]
}